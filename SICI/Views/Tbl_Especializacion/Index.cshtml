@model IEnumerable<SICI.Modelo.Tbl_Especializacion>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<p>
    @Html.ActionLink("Nueva especializacion", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Especializacion
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DescripcionEspecializacion)
        </td>
        <td>
            <a href="@Url.Action("Edit", new { id=item.IdEspecializacion})" class="btn btn-mini btn-info icon-Edit glyphicon glyphicon-pencil"> Editar</a>
            <a href="@Url.Action("Details", new { id=item.IdEspecializacion })" class="btn btn-mini btn-success icon-Edit glyphicon glyphicon-eye-open"> Detalles</a>
            <a href="@Url.Action("Delete", new { id=item.IdEspecializacion })" class="btn btn-mini btn-danger icon-Edit glyphicon glyphicon-trash"> Eliminar</a>
        </td>
    </tr>
}

</table>
