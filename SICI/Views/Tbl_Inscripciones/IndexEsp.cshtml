@model IEnumerable<SICI.Modelo.Tbl_Inscripciones>

@{
    ViewBag.Title = "IndexEsp";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Transferencias Disponibles</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tbl_Transferencias.NombreTransferencia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tbl_InformacionBasicaUsuario.Cedula)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tbl_Transferencias.NombreTransferencia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tbl_InformacionBasicaUsuario.Cedula)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdInscripcion }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdInscripcion }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdInscripcion })
        </td>
    </tr>
}

</table>
